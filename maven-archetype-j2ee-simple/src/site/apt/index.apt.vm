|       |   `-- java
|       `-- test
|           `-- java
|-- ejbs
|   |-- pom.xml
|   `-- src
|       |-- main
|       |   |-- java
|       |   `-- resources
|       |       `-- META-INF
|       |           `-- ejb-jar.xml
|       `-- test
|           `-- java
|-- primary-source
|   |-- pom.xml
|   `-- src
|       |-- main
|       |   `-- java
|       `-- test
|           `-- java
|-- projects
|   |-- pom.xml
|   `-- logging
|       |-- pom.xml
|       `-- src
|           |-- main
|           |   `-- java
|           `-- test
|               `-- java
`-- servlets
    |-- pom.xml
    `-- servlet
        |-- pom.xml
        `-- src
            |-- main
            |   `-- java
            |-- webapp
            |   |-- WEB-INF
            |   |   `-- web.xml
            |   `-- index.jsp
            `-- test
                `-- java

----

* Usage

 To generate a new project from this archetype, type:

----

mvn archetype:generate -DarchetypeGroupId=${project.groupId} -DarchetypeArtifactId=${project.artifactId} -DarchetypeVersion=${project.version}

